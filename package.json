{
  "name": "react-accessible-ellipsis",
  "version": "2012.1.2",
  "description": "Visibly truncate text (single or multi-line) with an ellipsis... without causing accessibility problems!",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "babel-build": "babel src --out-file build/index.js --extensions \".tsx\"",
    "prepublishOnly": "yarn build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/springload/react-accessible-ellipsis/"
  },
  "keywords": [
    "ellipsis",
    "dotdotdot",
    "dot dot dot",
    "truncate",
    "overflow",
    "accessibility",
    "a11y",
    "react",
    "text-overflow",
    "overflow",
    "ie11"
  ],
  "author": "Matthew Holloway",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/react": "^16.9.9",
    "jest": "^24.9.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "typescript": "^3.6.4"
  },
  "peerDependencies": {
    "react": ">16.8",
    "react-dom": ">16.8"
  },
  "babel": {
    "presets": [
      "@babel/typescript",
      "@babel/react"
    ]
  }
